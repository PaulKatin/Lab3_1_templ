@model IEnumerable<Lab3_1_templ.Models.GuestResponse>
@{
	ViewData["Title"] = "List of People";
}
<h1>@ViewData["Title"]</h1>

<body>
	<h2>Here is the list of people attending the party</h2>
	<table>
		<thead>
			<tr><th>Name</th><th>Email</th><th>Phone</th></tr>
		</thead>
		<tbody>
			@foreach (Lab3_1_templ.Models.GuestResponse r in Model!)
			{
				<tr>
					<td>@r.Name</td>
					<td>@r.Email</td>
					<td>@r.Phone</td>
				</tr>
			}
		</tbody>
	</table>
</body>